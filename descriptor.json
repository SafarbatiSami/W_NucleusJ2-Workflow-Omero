
{
  "name": "Biom3d-Workflow-Omero",
  "description": "3D segmentation using Biom3d",
  "container-image": {
    "image": "safarbatisami/w_biom3d-workflow-omero",
    "type": "singularity"
  },
  "command-line": "python wrapper.py CYTOMINE_HOST CYTOMINE_PUBLIC_KEY CYTOMINE_PRIVATE_KEY CYTOMINE_ID_PROJECT CYTOMINE_ID_SOFTWARE IMG_DIR MSK_DIR NUM_CLASSES DESC",
  "inputs": [
    {
      "id": "cytomine_host",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine host",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_public_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine public key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_private_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine private key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_id_project",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine project ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "cytomine_id_software",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Cytomine software ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "img_dir",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Image Directory",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    },
    {
      "id": "msk_dir",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Mask Directory",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    },
    {
      "id": "num_classes",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "ImagNumber of classes ",
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    },

    {
      "id": "desc",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Description of the dataset",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    }
  ],

  
  "schema-version": "cytomine-0.1"

}
